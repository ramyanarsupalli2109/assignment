	              //def mvnCMD = "${mvnHome}/bin/mvn"
	              //sh "${mvnCMD} package"
	              echo "building"
            }
        }

        stage('Code Test') {
            steps {
                  //def mvnHome = tool name: 'M3', type: 'maven'
	              //def mvnCMD = "${mvnHome}/bin/mvn"
	              //sh "${mvnCMD} test"
	              echo "testing"
               
            }
